<flow xmlns="http://www.springframework.org/schema/webflow"	
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"	
xsi:schemaLocation="http://www.springframework.org/schema/webflow
http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd"
parent="SeleccionCuentas/errorHandler"
start-state= "_jVoiceCheckInit" >

<on-start>
<evaluate expression="manageCuentas" result="flowScope.manageTexts"></evaluate>
<evaluate expression="text" result="flowScope.texto"></evaluate>

</on-start>


<action-state id="_jVoiceCheckInit">
	<evaluate expression="jVoiceArchData.initialized" />
		<transition on="no" to="_jVoiceInit"/>
		<transition to="invokeCtasCustPR"/>
	</action-state>

<view-state id="_jVoiceInit" view="_init" model="jVoiceArchData">
	<transition to="invokeCtasCustPR" >
		<set name="jVoiceArchData.initialized" value="true"/>
	</transition>
</view-state>
<end-state id="OkOneAcc" view="#{flowProcessor.getRenderer().getView()}">
	<output name="result" value="user.cuentasDebito[0]" />
</end-state>
<action-state id="invokeCtasCustPR">
   <evaluate expression="jWServiceCtasCustPR.invokeCtasCustPR()"
   />
   

	 <transition to="onlyOneAccount"></transition>
	
</action-state>

<action-state id="onlyOneAccount">
	<evaluate expression="user.cuentasDebito.size == 1" />
	<transition on="yes" to="OkOneAcc"/>
	<transition to="manageDebitAccountsTexts"/>
</action-state>
<action-state id="manageDebitAccountsTexts">
   <evaluate expression="flowScope.manageTexts.manageDebitAccountsTexts()"
   />
   

	 <transition to="selectCuentaDebito"></transition>
	
</action-state>
<action-state id="selectCuentaDebito">
	<on-entry>
		<evaluate expression="jVoiceArchInput" result="flashScope.selectCuentaDebito"></evaluate>
		<set name="flashScope.selectCuentaDebito.name" value="'selectCuentaDebito'" />
		
		<evaluate expression="jVoiceArchGrammar" result="flashScope.grammar1" />
		<set name="flashScope.grammar1.src" value="'builtin:dtmf/digits'"/>
		<set name="flashScope.grammar1.mode" value="'dtmf'"/>
		<set name="flashScope.grammar1.module" value="'SeleccionCuentas'"/>
		<evaluate expression="flashScope.selectCuentaDebito.grammars.add(flashScope.grammar1)" />
		
        <evaluate expression="jVoiceArchAudioItem" result="flashScope.selectCuentaDebitomainAudios1"/><evaluate expression="jVoiceArchWording" result="flashScope.selectCuentaDebitomainAudios1.wording"/>
        <set name="flashScope.selectCuentaDebitomainAudios1.wording.text" value="'' + texto.getTexto() + ''"/>
        <evaluate expression="flashScope.selectCuentaDebito.mainAudios.add(flashScope.selectCuentaDebitomainAudios1)"/>
        <evaluate expression="jVoiceArchAudioItem" result="flashScope.selectCuentaDebitonoMatchAudios1"/><set name="flashScope.selectCuentaDebitonoMatchAudios1.src" value="locutionProvider.getAudioSrc('puerto_rico-es-US-SAN-BC-NM','SeleccionCuentas')"/>
        <evaluate expression="jVoiceArchWording" result="flashScope.selectCuentaDebitonoMatchAudios1.wording"/>
        <set name="flashScope.selectCuentaDebitonoMatchAudios1.wording.text" value="'La opci칩n que usted seleccion칩 no es v치lida.'"/>
        <evaluate expression="flashScope.selectCuentaDebito.noMatchAudios.add(flashScope.selectCuentaDebitonoMatchAudios1)"/>
        <evaluate expression="jVoiceArchAudioItem" result="flashScope.selectCuentaDebitonoInputAudios1"/><set name="flashScope.selectCuentaDebitonoInputAudios1.src" value="locutionProvider.getAudioSrc('puerto_rico-es-US-SAN-BC-NI','SeleccionCuentas')"/>
        <evaluate expression="jVoiceArchWording" result="flashScope.selectCuentaDebitonoInputAudios1.wording"/>
        <set name="flashScope.selectCuentaDebitonoInputAudios1.wording.text" value="'No ha seleccionado una opci칩n.'"/>
        <evaluate expression="flashScope.selectCuentaDebito.noInputAudios.add(flashScope.selectCuentaDebitonoInputAudios1)"/>
	</on-entry>
	<evaluate expression="flowProcessor.process(flashScope.selectCuentaDebito)"/>			
	<transition to="render_selectCuentaDebito"/>
</action-state>

<view-state id="render_selectCuentaDebito" view="#{flowProcessor.getRenderer().getView()}" model="lastInputResult">
	<transition on="match" to="Switch"/>
</view-state>

<action-state id="Switch">
	<evaluate expression="lastInputResult.interpretation == 6" />
	<transition on="yes" to="manageDebitAccountsTexts"/>
	<transition to="OkMoreThanOneAcc"/>
</action-state>
<end-state id="OkMoreThanOneAcc" view="#{flowProcessor.getRenderer().getView()}">
	<output name="result" value="''" />
</end-state>
</flow> 
