<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.views">
      <view
            category="com.vectorsf.jvoice.ui.navigator.category"
            class="com.vectorsf.jvoice.ui.navigator.IVRNavigator"
            icon="res/navigator.png"
            id="com.vectorsf.jvoice.ui.navigator.ViewIVR"
            name="Navigator IVR">
      </view>
      <category
            id="com.vectorsf.jvoice.ui.navigator.category"
            name="IVR">
      </category>
   </extension>
   <extension
         point="org.eclipse.ui.navigator.viewer">
      <viewerActionBinding
            viewerId="com.vectorsf.jvoice.ui.navigator.ViewIVR">
         <includes>
            <actionExtension
                  pattern="com.vectorsf.jvoice.ui.navigator.ActionProvider">
            </actionExtension>
            <actionExtension
                  pattern="com.vectorsf.jvoice.ui.navigator.PortingActionProvider">
            </actionExtension>
            <actionExtension
                  pattern="com.vectorsf.jvoice.ui.navigator.PropertiesActionProvider">
            </actionExtension>
            <actionExtension
                  pattern="com.vectorsf.jvoice.ui.navigator.RefactorActionProvider">
            </actionExtension>
            <actionExtension
                  pattern="com.vectorsf.jvoice.ui.navigator.ResourceMgmtActionProvider">
            </actionExtension>
            <actionExtension
                  pattern="org.eclipse.ui.navigator.resources.NewActions">
            </actionExtension>
         </includes>
      </viewerActionBinding>
      <viewerContentBinding
            viewerId="com.vectorsf.jvoice.ui.navigator.ViewIVR">
         <includes>
            <contentExtension
                  isRoot="true"
                  pattern="com.vectorsf.jvoice.ui.navigator.navigatorContent">
            </contentExtension>
            <contentExtension
                  isRoot="false"
                  pattern="com.vectorsf.jvoice.base.maven.navigatorContent">
            </contentExtension>
            <contentExtension
                  pattern="com.vectorsf.jvoice.ui.navigator.emptyPackagesFilter">
            </contentExtension>
         </includes>
      </viewerContentBinding>
      <viewer
            viewerId="com.vectorsf.jvoice.ui.navigator.ViewIVR">
         <popupMenu
               allowsPlatformContributions="true"
               id="com.vectorsf.jvoice.ui.navigator.popup">
            <insertionPoint
                  name="group.new"
                  separator="true">
            </insertionPoint>
            <insertionPoint
                  name="group.open"
                  separator="true">
            </insertionPoint>
            <insertionPoint
                  name="group.openWith">
            </insertionPoint>
            <insertionPoint
                  name="group.edit"
                  separator="true">
            </insertionPoint>
            <insertionPoint
                  name="group.reorganize">
            </insertionPoint>
            <insertionPoint
                  name="group.port"
                  separator="true">
            </insertionPoint>
            <insertionPoint
                  name="group.build"
                  separator="true">
            </insertionPoint>
            <insertionPoint
                  name="group.properties"
                  separator="true">
            </insertionPoint>
            <insertionPoint
                  name="additions">
            </insertionPoint></popupMenu>
      </viewer>
   </extension>
   <extension
         point="org.eclipse.ui.navigator.navigatorContent">
      <navigatorContent
            activeByDefault="true"
            contentProvider="com.vectorsf.jvoice.ui.navigator.ivr.ContentProvider"
            icon="res/navigator.png"
            id="com.vectorsf.jvoice.ui.navigator.navigatorContent"
            labelProvider="com.vectorsf.jvoice.ui.navigator.ivr.LabelProvider"
            name="IVR Navigator"
            priority="normal">
         <triggerPoints>
            <instanceof
                  value="org.eclipse.emf.ecore.EObject">
            </instanceof>
         </triggerPoints>
         <dropAssistant
               class="com.vectorsf.jvoice.ui.navigator.drop.BeanCommonDropAdapterAssistant"
               id="com.vectorsf.jvoice.ui.navigator.drop.dropAssistant">
            <possibleDropTargets>
               <or>
                  <instanceof
                        value="com.vectorsf.jvoice.model.base.JVPackage">
                  </instanceof>
               </or></possibleDropTargets>
         </dropAssistant>
         <commonWizard
               type="new"
               wizardId="com.vectorsf.jvoice.ui.package.wizard">
            <enablement>
               <with
                     variable="selection">
                  <iterate
                        ifEmpty="true"
                        operator="and">
                     <or>
                        <instanceof
                              value="com.vectorsf.jvoice.model.base.JVProject">
                        </instanceof>
                        <instanceof
                              value="com.vectorsf.jvoice.model.base.JVPackage">
                        </instanceof>
                     </or>
                  </iterate>
               </with>
            </enablement>
         </commonWizard>
         <commonWizard
               type="new"
               wizardId="com.vectorsf.jvoice.ui.project.wizard">
            <enablement>
               <with
                     variable="selection">
                  <iterate
                        ifEmpty="true"
                        operator="and">
                     <or>
                        <instanceof
                              value="com.vectorsf.jvoice.model.base.JVProject">
                        </instanceof>
                        <instanceof
                              value="com.vectorsf.jvoice.model.base.JVPackage">
                        </instanceof>
                     </or>
                  </iterate>
               </with>
            </enablement>
         </commonWizard>
         <commonWizard
               type="new"
               wizardId="com.vectorsf.jvoice.ui.wizard.DiagramJVoice">
            <enablement>
               <with
                     variable="selection">
                  <iterate
                        ifEmpty="false"
                        operator="and">
                        <instanceof
                              value="com.vectorsf.jvoice.model.base.JVPackage">
                        </instanceof>
                  </iterate>
               </with>
            </enablement>
         </commonWizard>
         <commonWizard
               type="new"
               wizardId="com.vectorsf.jvoice.ui.wizard.wizardDSL">
            <enablement>
               <with
                     variable="selection">
                  <iterate
                        ifEmpty="false"
                        operator="and">
                     <instanceof
                           value="com.vectorsf.jvoice.model.base.JVPackage">
                     </instanceof>
                  </iterate>
               </with>
            </enablement>
         </commonWizard>
      </navigatorContent>
      <navigatorContent
            contentProvider="com.vectorsf.jvoice.ui.navigator.ivr.MavenContentProvider"
            id="com.vectorsf.jvoice.base.maven.navigatorContent"
            labelProvider="org.eclipse.jdt.internal.ui.navigator.JavaNavigatorLabelProvider"
            name="Maven navigator content"
            priority="low">
         <triggerPoints>
            <or>
               <instanceof
                     value="org.eclipse.jdt.core.IJavaElement">
               </instanceof>
               <instanceof
                     value="org.eclipse.jdt.internal.ui.packageview.ClassPathContainer">
               </instanceof>
               <instanceof
                     value="com.vectorsf.jvoice.model.base.JVProject">
               </instanceof>
            </or>
         </triggerPoints>
      </navigatorContent>
      <actionProvider
            class="com.vectorsf.jvoice.ui.navigator.CommonActionProviderIVR"
            id="com.vectorsf.jvoice.ui.navigator.ActionProvider">
         <enablement>
            <with
                  variable="selection">
               <and>
                  <iterate
                        ifEmpty="false"
                        operator="and">
                     <instanceof
                           value="com.vectorsf.jvoice.model.base.JVBean">
                     </instanceof>
                  </iterate>
               </and>
            </with>
         </enablement>
      </actionProvider>
      <actionProvider
            appearsBefore="org.eclipse.ui.navigator.resources.PropertiesActionProvider"
            class="org.eclipse.ui.internal.navigator.resources.actions.PortingActionProvider"
            id="com.vectorsf.jvoice.ui.navigator.PortingActionProvider">
         <enablement>
            <with
                  variable="selection">
               <and>
                  <iterate
                        ifEmpty="false"
                        operator="and">
                     <or>
                        <instanceof
                              value="com.vectorsf.jvoice.model.base.JVProject">
                        </instanceof>
                        <instanceof
                              value="com.vectorsf.jvoice.model.base.JVBean">
                        </instanceof>
                        <instanceof
                              value="com.vectorsf.jvoice.model.base.JVPackage">
                        </instanceof>
                     </or>
                  </iterate>
               </and>
            </with>
         </enablement>
      </actionProvider>
      <actionProvider
            class="org.eclipse.ui.internal.navigator.resources.actions.PropertiesActionProvider"
            id="com.vectorsf.jvoice.ui.navigator.PropertiesActionProvider">
         <enablement>
            <with
                  variable="selection">
               <and>
                  <iterate
                        ifEmpty="false"
                        operator="and">
                     <or>
                        <instanceof
                              value="com.vectorsf.jvoice.model.base.JVProject">
                        </instanceof>
                        <instanceof
                              value="com.vectorsf.jvoice.model.base.JVBean">
                        </instanceof>
                        <instanceof
                              value="com.vectorsf.jvoice.model.base.JVPackage">
                        </instanceof>
                     </or>
                  </iterate>
               </and>
            </with>
         </enablement>
      </actionProvider>
      <actionProvider
            appearsBefore="org.eclipse.ui.navigator.resources.PortingActions"
            class="org.eclipse.ui.internal.navigator.resources.actions.RefactorActionProvider"
            id="com.vectorsf.jvoice.ui.navigator.RefactorActionProvider">
         <enablement>
            <with
                  variable="selection">
               <and>
                  <iterate
                        ifEmpty="false"
                        operator="and">
                     <or>
                        <instanceof
                              value="com.vectorsf.jvoice.model.base.JVProject">
                        </instanceof>
                        <instanceof
                              value="com.vectorsf.jvoice.model.base.JVBean">
                        </instanceof>
                        <instanceof
                              value="com.vectorsf.jvoice.model.base.JVPackage">
                        </instanceof>
                     </or>
                  </iterate>
               </and>
            </with>
         </enablement>
      </actionProvider>
      <actionProvider
            appearsBefore="org.eclipse.ui.navigator.resources.PropertiesActionProvider"
            class="org.eclipse.ui.internal.navigator.resources.actions.ResourceMgmtActionProvider"
            id="com.vectorsf.jvoice.ui.navigator.ResourceMgmtActionProvider">
         <enablement>
            <with
                  variable="selection">
               <and>
                  <iterate
                        ifEmpty="false"
                        operator="and">
                     <or>
                        <instanceof
                              value="com.vectorsf.jvoice.model.base.JVProject">
                        </instanceof>
                        <instanceof
                              value="com.vectorsf.jvoice.model.base.JVBean">
                        </instanceof>
                        <instanceof
                              value="com.vectorsf.jvoice.model.base.JVPackage">
                        </instanceof>
                     </or>
                  </iterate>
               </and>
            </with>
         </enablement>
      </actionProvider>
      <commonFilter
            activeByDefault="true"
            class="com.vectorsf.jvoice.ui.navigator.filter.EmptyPackagesFilter"
            id="com.vectorsf.jvoice.ui.navigator.emptyPackagesFilter"
            name="Empty packages filter"
            visibleInUI="true">
      </commonFilter>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="true"
            locationURI="popup:com.vectorsf.jvoice.ui.navigator.popup?after=group.edit">
         <command
               commandId="org.eclipse.ui.edit.copy"
               label="Copy"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <with
                     variable="selection">
               </with>
            </visibleWhen>
         </command>
         <command
               commandId="org.eclipse.ui.edit.paste"
               label="Paste"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <with
                     variable="selection">
                  <count
                        value="1">
                  </count>
               </with>
            </visibleWhen>
         </command>
         <command
               commandId="org.eclipse.ui.edit.delete"
               label="Delete"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <with
                     variable="selection">
                  <iterate
                        ifEmpty="false"
                        operator="and">
                     <or>
                        <instanceof
                              value="com.vectorsf.jvoice.model.base.JVBean">
                        </instanceof>
                        <instanceof
                              value="com.vectorsf.jvoice.model.base.JVPackage">
                        </instanceof>
                        <instanceof
                              value="com.vectorsf.jvoice.model.base.JVProject">
                        </instanceof>
                     </or>
                  </iterate>
               </with>
            </visibleWhen>
         </command>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.core.runtime.adapters">
      <factory
            adaptableType="com.vectorsf.jvoice.model.base.JVProject"
            class="com.vectorsf.jvoice.ui.navigator.AdapterProject">
         <adapter
               type="org.eclipse.core.resources.IProject">
         </adapter>
         <adapter
               type="org.eclipse.core.resources.mapping.ResourceMapping">
         </adapter>
         <adapter
               type="org.eclipse.core.resources.IResource">
         </adapter>
      </factory>
      <factory
            adaptableType="com.vectorsf.jvoice.model.base.JVBean"
            class="com.vectorsf.jvoice.ui.navigator.AdapterBean">
         <adapter
               type="org.eclipse.core.resources.IFile">
         </adapter>
         <adapter
               type="org.eclipse.core.resources.mapping.ResourceMapping">
         </adapter>
         <adapter
               type="org.eclipse.core.resources.IResource">
         </adapter>
      </factory>
      <factory
            adaptableType="com.vectorsf.jvoice.model.base.JVPackage"
            class="com.vectorsf.jvoice.ui.navigator.AdapterPackage">
         <adapter
               type="org.eclipse.core.resources.IFolder">
         </adapter>
         <adapter
               type="org.eclipse.core.resources.mapping.ResourceMapping">
         </adapter>
         <adapter
               type="org.eclipse.core.resources.IResource">
         </adapter>
      </factory>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="com.vectorsf.jvoice.ui.navigator.handler.DeleteHandler"
            commandId="org.eclipse.ui.edit.delete">
         <activeWhen>
            <with
                  variable="selection">
               <iterate
                     operator="and">
                  <or>
                     <instanceof
                           value="com.vectorsf.jvoice.model.base.JVPackage">
                     </instanceof>
                     <instanceof
                           value="com.vectorsf.jvoice.model.base.JVBean">
                     </instanceof>
                     <instanceof
                           value="com.vectorsf.jvoice.model.base.JVProject">
                     </instanceof>
                  </or>
               </iterate>
            </with>
         </activeWhen>
      </handler>
      <handler
            class="com.vectorsf.jvoice.ui.navigator.handler.PasteHandler"
            commandId="org.eclipse.ui.edit.paste">
         <activeWhen>
            <with
                  variable="selection">
               <iterate
                     operator="and">
                  <or>
                     <instanceof
                           value="com.vectorsf.jvoice.model.base.JVPackage">
                     </instanceof>
                     <instanceof
                           value="com.vectorsf.jvoice.model.base.JVProject">
                     </instanceof>
                  </or>
               </iterate>
            </with>
         </activeWhen>
      </handler>
      <handler
            class="com.vectorsf.jvoice.ui.navigator.handler.CopyHandler"
            commandId="org.eclipse.ui.edit.copy">
         <activeWhen>
            <with
                  variable="selection">
               <iterate
                     operator="and">
                  <or>
                     <instanceof
                           value="com.vectorsf.jvoice.model.base.JVPackage">
                     </instanceof>
                     <instanceof
                           value="com.vectorsf.jvoice.model.base.JVBean">
                     </instanceof>
                  </or>
               </iterate>
            </with>
         </activeWhen>
      </handler>
   </extension>

</plugin>
